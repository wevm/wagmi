{
  "name": "@wagmi/test",
  "description": "Test utils for wagmi",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wevm/wagmi.git",
    "directory": "packages/test"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "zile --project tsconfig.build.json",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "preconstruct": "zile dev --project tsconfig.build.json",
    "test:build": "publint --strict"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "react/**",
    "vue/**"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/exports/index.js",
  "types": "./dist/exports/index.d.ts",
  "typings": "./dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "src": "./src/exports/index.ts",
      "types": "./dist/exports/index.d.ts",
      "default": "./dist/exports/index.js"
    },
    "./react": {
      "src": "./src/exports/react.ts",
      "types": "./dist/exports/react.d.ts",
      "default": "./dist/exports/react.js"
    },
    "./vue": {
      "src": "./src/exports/vue.ts",
      "types": "./dist/exports/vue.d.ts",
      "default": "./dist/exports/vue.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "react": [
        "./dist/types/exports/react.d.ts"
      ],
      "vue": [
        "./dist/types/exports/vue.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "@tanstack/vue-query": ">=5.0.0",
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "@wagmi/core": "workspace:*",
    "@wagmi/vue": "workspace:*",
    "prool": "^0.0.24",
    "react": ">=18",
    "react-dom": ">=18",
    "typescript": ">=5.0.4",
    "viem": "2.x",
    "vue": ">=3",
    "wagmi": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "@tanstack/vue-query": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tanstack/react-query": "catalog:",
    "@tanstack/vue-query": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@wagmi/core": "workspace:*",
    "@wagmi/vue": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:",
    "vue": "catalog:",
    "wagmi": "workspace:*"
  },
  "contributors": [
    "awkweb.eth <t@wevm.dev>",
    "jxom.eth <j@wevm.dev>"
  ],
  "funding": "https://github.com/sponsors/wevm",
  "keywords": [
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ],
  "module": "./dist/exports/index.js"
}