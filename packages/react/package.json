{
  "name": "wagmi",
  "description": "React Hooks for Ethereum",
  "version": "2.18.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wevm/wagmi.git",
    "directory": "packages/react"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "zile --project tsconfig.build.json",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo actions chains codegen connectors experimental query",
    "preconstruct": "zile dev --project tsconfig.build.json",
    "test:build": "publint --strict && attw --pack --ignore-rules cjs-resolves-to-esm"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "/actions",
    "/chains",
    "/codegen",
    "/connectors",
    "/experimental",
    "/query"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/exports/index.js",
  "types": "./dist/exports/index.d.ts",
  "typings": "./dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "src": "./src/exports/index.ts",
      "types": "./dist/exports/index.d.ts",
      "default": "./dist/exports/index.js"
    },
    "./actions": {
      "src": "./src/exports/actions.ts",
      "types": "./dist/exports/actions.d.ts",
      "default": "./dist/exports/actions.js"
    },
    "./actions/experimental": {
      "src": "./src/exports/actions/experimental.ts",
      "types": "./dist/exports/actions/experimental.d.ts",
      "default": "./dist/exports/actions/experimental.js"
    },
    "./chains": {
      "src": "./src/exports/chains.ts",
      "types": "./dist/exports/chains.d.ts",
      "default": "./dist/exports/chains.js"
    },
    "./codegen": {
      "src": "./src/exports/codegen.ts",
      "types": "./dist/exports/codegen.d.ts",
      "default": "./dist/exports/codegen.js"
    },
    "./connectors": {
      "src": "./src/exports/connectors.ts",
      "types": "./dist/exports/connectors.d.ts",
      "default": "./dist/exports/connectors.js"
    },
    "./experimental": {
      "src": "./src/exports/experimental.ts",
      "types": "./dist/exports/experimental.d.ts",
      "default": "./dist/exports/experimental.js"
    },
    "./query": {
      "src": "./src/exports/query.ts",
      "types": "./dist/exports/query.d.ts",
      "default": "./dist/exports/query.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "react": ">=18",
    "typescript": ">=5.0.4",
    "viem": "2.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@wagmi/connectors": "workspace:*",
    "@wagmi/core": "workspace:*",
    "use-sync-external-store": "1.4.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/use-sync-external-store": "^0.0.6",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "contributors": [
    "awkweb.eth <t@wevm.dev>",
    "jxom.eth <j@wevm.dev>"
  ],
  "funding": "https://github.com/sponsors/wevm",
  "keywords": [
    "wagmi",
    "react",
    "hooks",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ],
  "module": "./dist/exports/index.js"
}